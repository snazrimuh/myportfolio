# ─── Database (PostgreSQL) ──────────────────────────────────────────
POSTGRES_DB=portfolio
POSTGRES_USER=portfolio_user
POSTGRES_PASSWORD=portfolio_pass

# ─── Backend ──────────────────────────────────────────────
JWT_SECRET=ganti-dengan-string-random-panjang-minimal-64-karakter
JWT_EXPIRES_IN=1d

# ─── Admin default (digunakan seed) ───────────────────────
ADMIN_EMAIL=admin@portfolio.com
ADMIN_PASSWORD=admin123

# ─── URLs ─────────────────────────────────────────────────
# URL frontend yang diakses publik (untuk CORS backend)
FRONTEND_URL=http://localhost

# Port nginx (default 80)
PORT=80
